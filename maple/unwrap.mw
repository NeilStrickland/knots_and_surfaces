<Group labelreference="L153" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i from 0 to 1 by 0.01 do 
 t0 := evalf(0.02 * Pi);
 a0 := f(0): a1 := g(0): a2 := (1-i) *~ a0 +~ i *~ a1; 
 b0 := f(t0): b1 := g(t0): b2 := (1-i) *~ b0 +~ i *~ b1; 
 P0[i] := display(
  PP, line(a0,a2,thickness=3), line(a2,b2,thickness=3), line(b2,b0,thickness=3),
  spacecurve(f(t),t=t0..2*Pi,numpoints=500,thickness=3),
  orientation=oo,axes=none,scaling=constrained
 );
od:</Text-field>
</Input>
</Group>
<Group labelreference="L35" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i from 0.01 to 1 by 0.001 do
 a := evalf(i*2*Pi);
 P1[i] := display(
  spacecurve(g(t),t=0..2*Pi,colour=gray,thickness=1),
  spacecurve(f(t),t=0..2*Pi,numpoints=500,colour=gray,thickness=1),
  line(f(0),g(0),thickness=3), 
  spacecurve(g(t),t=0..a,thickness=3),
  line(g(a),f(a),thickness=3),
  spacecurve(f(t),t=a..2*Pi,numpoints=500,thickness=3),
  orientation=oo,axes=none,scaling=constrained
 );
od:</Text-field>
</Input>
